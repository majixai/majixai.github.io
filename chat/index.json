{
  "appName": "Gemini Financial Modeling Chat",
  "appVersion": "1.3.0",
  "apiConfig": {
    "modelName": "gemini-1.5-flash-latest",
    "apiKeyPlaceholder": "YOUR_API_KEY_HERE"
