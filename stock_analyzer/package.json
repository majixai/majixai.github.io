{
  "name": "stock-analyzer",
  "version": "0.1.0",
  "description": "Stock projection analyzer application",
  "main": "dist/bundle.js",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "esbuild index.tsx --bundle --outfile=dist/bundle.js --minify --sourcemap --format=esm --platform=browser --external:marked --external:@google/genai",
