    "esbuild": "^0.20.2",
    "@types/node": "^20.12.7",
    "@types/marked": "^6.0.0"
  },
  "dependencies": {
    "marked": "^12.0.2", 
    "@google/genai": "^0.7.0"
  },
  "license": "Apache-2.0"
}
